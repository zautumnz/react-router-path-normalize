{
  "name": "react-router-path-normalize",
  "description": "Normalize paths for React Router.",
  "version": "3.0.17",
  "author": {
    "name": "Autumn Z"
  },
  "license": "LGPL-3.0",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "babel src/index.js --out-file lib/index.js",
    "clean": "rm -rf lib && mkdir -p lib",
    "cover": "nyc npm run test:tape",
    "format": "prettier --write src/*.js",
    "prebuild": "sortpack && run-s -s test:lint cover clean",
    "preversion": "npm run build",
    "test": "run-s -s test:lint test:tape",
    "test:lint": "eslint -c .eslintrc.json src/",
    "test:tape": "babel-tape-runner src/index.test.js"
  },
  "homepage": "https://github.com/zautumnz/react-router-path-normalize#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/zautumnz/react-router-path-normalize.git"
  },
  "bugs": "https://github.com/zautumnz/react-router-path-normalize/issues",
  "keywords": [
    "normalize",
    "path",
    "react",
    "react-router",
    "react-router-path-normalize",
    "router"
  ],
  "dependencies": {
    "path-normalize": "6.0.2"
  },
  "devDependencies": {
    "@babel/cli": "7.12.10",
    "@babel/core": "7.12.10",
    "@babel/preset-env": "7.12.10",
    "@babel/preset-react": "7.12.10",
    "babel-eslint": "10.1.0",
    "babel-plugin-add-module-exports": "1.0.4",
    "babel-tape-runner": "3.0.0",
    "eslint": "7.15.0",
    "npm-run-all": "4.1.5",
    "nyc": "15.1.0",
    "prettier": "2.2.1",
    "react": "16.12.0",
    "react-dom": "16.13.1",
    "react-router": "5.1.2",
    "react-router-dom": "5.1.2",
    "react-test-renderer": "16.13.1",
    "sortpack": "2.1.11",
    "tape": "5.0.1"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "nyc": {
    "include": [
      "src/index.js"
    ],
    "instrument": true,
    "reporter": [
      "text",
      "json",
      "html"
    ],
    "require": [
      "@babel/register"
    ],
    "sourceMap": true
  },
  "peerDependencies": {
    "react": "^16.0.0",
    "react-router": "^4.0.0 || ^5.0.0"
  }
}
